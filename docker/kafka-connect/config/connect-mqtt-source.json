{
	"name": "mqtt-source",
	"config": {
		"connector.class": "io.confluent.connect.mqtt.MqttSourceConnector",
		"tasks.max": "1",
		"mqtt.server.uri": "tcp://mosquitto:1883",
		"mqtt.topics": "senml.#",
		"kafka.topic": "senml",
		"confluent.topic.bootstrap.servers": "kafka:9092",
		"confluent.topic.replication.factor": "1",
		"confluent.license": ""
	}
}
